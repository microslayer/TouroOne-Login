{
  "manifest_version": 2,

  "name": "TouroOne Login",
  "description": "Automatically logs TouroOne users into TouroOne.",
  "version": "1.1",
  "background": { 
   "page":"bg.html"
   },
   "content_scripts": [{
    "matches": ["https://touroone.touro.edu/sso/login*", "https://touroone.portal.touro.edu/" ],
    "js": [
    "content.js", 
    "jquery.js"
    ]
    }], 
    "icons": { "19": "images/blackboard-logo.png",
    "48": "images/tourologo.jpg" }, 

    "browser_action": {
      "default_title": "Touro Blackboard Login", 
      "default_popup": "popup.html", 
      "default_icon": "images/blackboard-logo.png"
      }, 

      "permissions": [
      "storage",
      "nativeMessaging", 
      "tabs"
      ]
    }